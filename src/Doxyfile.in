PROJECT_NAME           = @PACKAGE_NAME@
PROJECT_NUMBER         = @PACKAGE_VERSION@
PROJECT_BRIEF          = "Transcription start site (TSS) hidden Markov model, built against the general hidden Markov model (GHMM) library"
OUTPUT_DIRECTORY       = doc
OPTIMIZE_OUTPUT_FOR_C  = YES
FILE_PATTERNS          = *.c \
                         *.h
EXCLUDE_SYMBOLS        = attribute_visible __attribute__
WARN_IF_UNDOCUMENTED   = YES
WARN_IF_DOC_ERROR      = YES
WARN_NO_PARAMDOC       = YES
QUIET                  = YES
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
GENERATE_MAN           = YES
CLASS_DIAGRAMS         = NO
INCLUDE_PATH           = @R_INCLUDE@ @GHMM_INCLUDE@
MACRO_EXPANSION        = YES
SEARCH_INCLUDES        = YES
ENABLE_PREPROCESSING   = YES
SKIP_FUNCTION_MACROS   = NO
